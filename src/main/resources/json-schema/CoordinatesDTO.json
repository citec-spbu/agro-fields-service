{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CoordinatesDTO",
  "type": "object",
  "properties": {
    "longitude": {
      "type": "number",
      "format": "double",
      "minimum": -180,
      "maximum": 180,
      "description": "Must be filled"
    },
    "latitude": {
      "type": "number",
      "format": "double",
      "minimum": -90,
      "maximum": 90,
      "description": "Must be filled"
    }
  },
  "required": ["longitude", "latitude"]
}